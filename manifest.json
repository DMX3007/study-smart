{
    "name" : "EDUCATOR",
    "version" : "1.0.0",
    "manifest_version" : 3,
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*" ],
        "js": ["content-script.js"]
      }
    ],
      "action": {
        "default_popup": "popup.html"
      },
    "host_permissions": ["https://*/*, http://*/*, <all_urls>"]
}